  "license": "MIT",
  "dependencies": {
    "@google/maps": "^0.5.5",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "express-validator": "^6.1.0",
    "google-auth-library": "^4.2.5",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.6.2",
    "node-fetch": "^2.6.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "validator": "^11.0.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.1",
    "nodemon": "^1.19.1"
  }
}
